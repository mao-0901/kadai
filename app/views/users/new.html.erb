<h1>Users#new</h1>

<%= form_for(@user, url: users_path, html: { medhod: :post }) do |f| %>
<div>
    <%= f.label :username %>
    <%= f.text_field :username %>
</div>
<div>
    <%= f.label :email %>
    <%= f.text_field :email %>
</div>

<div>
    <%= f.label  :password %>
    <%= f.text_field  :password %>
</div>

<div>
    <%= f.label  :password_confirmation %>
    <%= f.text_field  :password_confirmation %>
</div>

<div>
    <%= f.submit %>
</div>
<% end %>